                          </div>
                          <div class="nav-dropdown__content">
                            {% if informations %}
                            <ul class="list-unstyled mobile-information">
                              <li class="mobile-info__title"><strong>{{ text_info }}</strong></li>
                              {% for information in informations %}
                              <li class="mobile-info__item">				
                                <a href="{{ information.href }}" class="mobile-info__link link-bottom">{{ information.title }}</a>						
                              </li>
                              {% endfor %}	
                            </ul>
                            {% endif %}
                            <ul class="list-unstyled mobile-phone">
                              <li class="mobile-info__title"><strong>{{ text_phone }}</strong></li>
                              <li class="mobile-info__item"><a href="tel:{{ tel }}" class="mobile-info__link">{{ telephone }}</a></li>
                            </ul>
                            <ul class="list-unstyled mobile-address">
                              <li class="mobile-info__title"><strong>{{ text_address }}</strong></li>
                              <li class="mobile-info__item"><span class="mobile-info__link">{{ address }}</span></li>
                            </ul>
                            {% if open %}
                            <ul class="list-unstyled mobile-job">
                              <li class="mobile-info__title"><strong>{{ text_job }}</strong></li>
                              <li class="mobile-info__item"><span class="mobile-info__link">{{ open }}</span></li>
                            </ul>
                            {% endif %}
                            <div class="mobile-menu__wishlist">
                              <a href="{{ wishlist }}" class="mobile-wishlist__link wishlist-link relative wishlist-total">{{ text_wishlist }}</a>
                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="mobile-menu__login">
